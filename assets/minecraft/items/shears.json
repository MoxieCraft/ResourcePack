{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "netherite_shears",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/netherite_shears"
        }
      },
      {
        "when": "assassin_shears",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/assassin_shears"
        }
      },
      {
        "when": "assassin_shears_alt",
        "model": {
          "type": "minecraft:model",
          "model": "moxiecraft:item/assassin_shears_alt"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "minecraft:item/shears"
    }
  }
}